{
    "name": "devboxes",
    "image": "public.ecr.aws/yblew/y-dev-master:latest",
    "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance",
        "dbaeumer.vscode-eslint"
    ],
    "settings": {
        "python.linting.enabled": true,
        "python.linting.flake8Enabled": true,
        "python.formatting.provider": "black",
        "python.defaultInterpreterPath": " /workspace/configs/virenv/bin/python",
        "python.formatting.autopep8Path": " /workspace/configs/virenv/bin/autopep8",
        "python.formatting.blackPath": " /workspace/configs/virenv/bin/black",
        "python.formatting.yapfPath": " /workspace/configs/virenv/bin/yapf",
        "python.linting.banditPath": " /workspace/configs/virenv/bin/bandit",
        "python.linting.flake8Path": " /workspace/configs/virenv/bin/flake8",
        "python.linting.mypyPath": " /workspace/configs/virenv/bin/mypy",
        "python.linting.pylintPath": " /workspace/configs/virenv/bin/pylint",
        "python.linting.pydocstylePath": " /workspace/configs/virenv/bin/pydocstyle",
        "python.linting.pycodestylePath": " /workspace/configs/virenv/bin/pycodestyle"
    },
    "workspaceFolder": "/workspace",
    "workspaceMount": "source=${localEnv:USER_ID},target=/workspace,type=volume",
    "remoteUser": "vscode"
}